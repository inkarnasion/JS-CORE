{{> header}}
<div id="cinema">
  <h1>All Movies</h1>
  <form action="#/allMovies" method="GET">
    <label>Search genre:</label>
    <input type="text" name="search">
    <input type="submit" value="Search">
  </form>
  <div class="movies">
    {{#each movies}}
    {{> movieView}}
    {{/each}}
  </div>
</div>
{{> footer}}